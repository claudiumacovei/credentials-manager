<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity Credential.
    -->
    <changeSet id="20220127093226-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="identity_provider_id"
                                 baseTableName="credential"
                                 constraintName="fk_credential__identity_provider_id"
                                 referencedColumnNames="id"
                                 referencedTableName="identity_provider"/>

        <addForeignKeyConstraint baseColumnNames="credential_id"
                                 baseTableName="rel_credential__service_providers"
                                 constraintName="fk_rel_credential__service_providers__credential_id"
                                 referencedColumnNames="id"
                                 referencedTableName="credential"/>

        <addForeignKeyConstraint baseColumnNames="service_providers_id"
                                 baseTableName="rel_credential__service_providers"
                                 constraintName="fk_rel_credential__service_providers__service_providers_id"
                                 referencedColumnNames="id"
                                 referencedTableName="service_provider"/>
    </changeSet>
</databaseChangeLog>
